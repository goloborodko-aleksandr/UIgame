<log4net>
    <appender name="UnityConsoleAppender" type="Engine.AxGridUnityTools.UnityAppender, Assembly-CSharp">
        <layout type="log4net.Layout.PatternLayout">
            <conversionPattern value="%level %logger - %message%newline" />
        </layout>
    </appender>

    <root>
        <level value="DEBUG" />
        <appender-ref ref="UnityConsoleAppender" />
    </root>
</log4net>